{
    "name": "pakacuda/cakephp-fixture-factories",
    "description": "An alternative to the CakePHP fixture approach",
    "authors": [
        {
            "name": "Nicolas Masson",
            "email": "nicolas@b-projects.tech"
        },
        {
            "name": "Juan Pablo RamÃ¬rez",
            "email": "pabloelcolombiano@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "cakephp/cakephp": "^3.6",
        "cakephp/migrations": "^2.3",
        "fzaninotto/faker": "^1.9@dev"
    },
    "require-dev": {
        "cakephp/bake": "^1.5",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "TestFixtureFactories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestFixtureFactories\\Test\\": "tests",
            "TestApp\\": "tests/TestApp/src",
            "TestPlugin\\": "tests/TestApp/plugins/TestPlugin/src"
        }
    },
    "scripts": {
        "run-tests": "./vendor/bin/phpunit"
    }
}
